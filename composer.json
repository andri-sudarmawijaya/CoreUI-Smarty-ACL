{
    "name": "andri-sudarmawijaya/coreui-smarty-acl",
    "description": "Theme Core UI for CodeIgniter with Auth by Smarty ACL",
    "type": "codeigniter-views",
    "require": {
        "andri-sudarmawijaya/smartyacl": "1.0.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andri Sudarmawijaya",
            "email": "andri.sudarmawijaya@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AndriSudarmawijaya\\CoreuiSmartyAcl\\": "src/"
        }
    },
    "scripts" : {
        "post-install-cmd": [
            "AndriSudarmawijaya\\CoreuiSmartyAcl\\Installer::postInstall"
        ],
        "post-update-cmd": [
            "AndriSudarmawijaya\\CoreuiSmartyAcl\\Installer::postUpdate"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
	}
}